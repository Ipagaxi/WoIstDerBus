{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../build",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "widb-client",
  "mainBinaryName": "widb-client",
  "version": "0.1.0",
  "identifier": "com.tauri.widb",
  "plugins": {},
  "app": {
    "security": {
      "csp": "default-src 'self'; connect-src 'self' https://*.tile.openstreetmap.org https://*.tile.leafletjs.com; img-src 'self' data: https://*.tile.openstreetmap.org https://*.tile.leafletjs.com;"
    },
    "windows": [
      {
        "title": "widb-client",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true
      }
    ]
  }
}